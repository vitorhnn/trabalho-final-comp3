<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <display-name>Trabalho final de Computação 3</display-name>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>br.net.hnn.ufrrj.comp3_trabalho_final.dominio.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SolicitacaoMuseuServlet</servlet-name>
        <servlet-class>br.net.hnn.ufrrj.comp3_trabalho_final.dominio.solicitacaomuseu.SolicitacaoMuseuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CriarMuseuServlet</servlet-name>
        <servlet-class>br.net.hnn.ufrrj.comp3_trabalho_final.dominio.criarmuseu.CriarMuseuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CriarGestorServlet</servlet-name>
        <servlet-class>br.net.hnn.ufrrj.comp3_trabalho_final.dominio.criargestor.CriarGestorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CriarGestorServlet</servlet-name>
        <url-pattern>/criar-gestor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CriarMuseuServlet</servlet-name>
        <url-pattern>/criar-museu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SolicitacaoMuseuServlet</servlet-name>
        <url-pattern>/solicitacao-museu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <listener>
        <listener-class>br.net.hnn.ufrrj.comp3_trabalho_final.dominio.StartupListener</listener-class>
    </listener>
</web-app>
